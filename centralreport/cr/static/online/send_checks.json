{
    "date": "{{ date }}",
    "checks": {
        "load": {
            "uptime": {{ load_uptime }},
            "lastMinuteAverage": {{ load_last1m }},
            "lastFiveMinutesAverage": {{ load_last5m }},
            "lastFifteenMinutesAverage": {{ load_last15m }}
        },
        "cpu": {
            "idlePercentage": {{ cpu_idle }},
            "systemPercentage": {{ cpu_system }},
            "userPercentage": {{ cpu_user }}
        },
        "memory": {
            "total": {{ memory_total }},
            "active": {{ memory_active }},
            "inactive": {{ memory_inactive }},
            "resident": {{ memory_resident }},
            "free": {{ memory_free }},
            "swapSize": {{ swap_size }},
            "usedSwap": {{ swap_used }},
            "freeSwap": {{ swap_free }}
        },
        "disks": [
            {% for disk in disks %}
            {
                "name":"{{ disk.name }}",
                "displayName":"{{ disk.display_name }}",
                "uuid":"{{ disk.uuid }}",
                "size":{{ disk.size }},
                "usedSpace":{{ disk.used }},
                "freeSpace":{{ disk.free }}
            },
            {% endfor %}
        ]
    }
}
